{
  "plugins": ["prettier"],

  "extends": ["airbnb", "plugin:prettier/recommended"],

  "env": {
    "browser": true,
    "node": true
  },

  "rules": {
    "no-new": "on",
    "no-console": "off",
    "no-param-reassign": "off",
    "no-alert": "off",
    "no-undef": "off",
    "import/extensions": "off",
    "prefer-template": "off",
    "max-depth": ["error", 2],
    "max-lines-per-function": ["error", 15],
    "eol-last": ["error", "always"]
  }
}
